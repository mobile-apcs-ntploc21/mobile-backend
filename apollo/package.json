{
  "name": "apollo",
  "version": "1.0.0",
  "main": "src/server.ts",
  "_moduleAliases": {
    "@": "dist",
    "@models": "dist/models",
    "@resolvers": "dist/graphql/resolvers"
  },
  "scripts": {
    "lint": "eslint \"./**/*.ts\"",
    "prettier": "prettier --check \"./**/*.{ts,json}\"",
    "prettier-fix": "prettier --write \"./**/*.{ts,json}\"",
    "build": "tsc",
    "start": "node -r module-alias/register dist/server.js",
    "dev": "ts-node-dev -r tsconfig-paths/register src/server.ts"
  },
  "dependencies": {
    "@graphql-tools/merge": "^9.0.4",
    "@graphql-tools/schema": "^10.0.4",
    "@graphql-tools/utils": "^10.3.2",
    "@types/amqplib": "^0.10.6",
    "amqplib": "^0.10.5",
    "apollo-server": "^3.13.0",
    "apollo-server-core": "^3.13.0",
    "apollo-server-express": "^3.13.0",
    "graphql": "^16.9.0",
    "graphql-resolvers": "^0.4.2",
    "graphql-scalars": "^1.23.0",
    "graphql-subscriptions": "^2.0.0",
    "graphql-tools": "^9.0.1",
    "graphql-ws": "^5.16.0",
    "mongoose": "^8.4.3",
    "short-unique-id": "^5.2.0",
    "validator": "^13.12.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/graphql-resolvers": "^0.3.2",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.9",
    "@types/validator": "^13.12.0",
    "@types/ws": "^8.5.11"
  }
}
